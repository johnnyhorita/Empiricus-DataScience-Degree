   "source": [
    "from pynput.keyboard import Key, Listener\n",
    "  \n",
    "def show(key):\n",
    "  \n",
    "    print('\\nYou Entered {0}'.format( key))\n",
    "  \n",
    "    if key == Key.delete:\n",
    "        # Stop listener\n",
    "        return False\n",
    "  \n",
    "# Collect all event until released\n",
    "with Listener(on_press = show) as listener:   \n",
    "    listener.join()"
   ]
  }
